{
  "name": "benchmarks",
  "private": true,
  "type": "module",
  "scripts": {
    "bench": "pnpm run build && bun run runner.ts",
    "build": "rm -rf dist && vite build",
    "build:dev": "pnpm run build",
    "build:prod": "pnpm run build --base '/benchmarks/'",
    "dev": "vite --port 4555 --host 0.0.0.0",
    "format": "cd .. && pnpm format",
    "preview": "pnpm run serve",
    "serve": "sirv ./dist",
    "start": "pnpm run serve",
    "typecheck": "./typecheck.sh"
  },
  "dependencies": {
    "preact": "^10.28.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "remix": "3.0.0-alpha.1",
    "sirv-cli": "^3.0.1",
    "solid-js": "^1.9.11",
    "svelte": "^5.48.3",
    "vani-local": "workspace:*",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "catalog:",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "clsx": "^2.1.1",
    "playwright": "catalog:",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vite": "catalog:",
    "vite-plugin-solid": "^2.11.10"
  },
  "benchmarks": {
    "version": "3.0.0",
    "port": 8778,
    "cpuThrottling": 4,
    "benchmarkRuns": 5,
    "warmupRuns": 2,
    "resultsRetention": 30,
    "frameworks": [
      {
        "id": "vanilla",
        "name": "Vanilla JS",
        "version": "4.0.2",
        "path": "frameworks/vanilla",
        "implementationNotes": "no-keyed, no-vdom, no-jsx, no-memo, dom-caching"
      },
      {
        "id": "vani",
        "name": "Vani",
        "package": "@vanijs/vani",
        "path": "frameworks/vani",
        "implementationNotes": "keyed, no-vdom, no-jsx, no-memo, attr-only-updates"
      },
      {
        "id": "react",
        "name": "React",
        "package": "react",
        "path": "frameworks/react",
        "implementationNotes": "keyed, vdom, jsx, no-memo, use-state"
      },
      {
        "id": "preact",
        "name": "Preact",
        "package": "preact",
        "path": "frameworks/preact",
        "implementationNotes": "keyed, vdom, jsx, no-memo, use-state"
      },
      {
        "id": "remix",
        "name": "Remix",
        "package": "remix",
        "path": "frameworks/remix",
        "implementationNotes": "keyed, vdom, jsx, no-memo"
      },
      {
        "id": "solid",
        "name": "Solid",
        "package": "solid-js",
        "path": "frameworks/solid",
        "implementationNotes": "keyed, no-vdom, jsx, no-memo, signals, selectors"
      },
      {
        "id": "svelte",
        "name": "Svelte",
        "package": "svelte",
        "path": "frameworks/svelte",
        "implementationNotes": "keyed, no-vdom, no-jsx, no-memo, runes, state-raw"
      },
      {
        "id": "vue",
        "name": "Vue",
        "package": "vue",
        "path": "frameworks/vue",
        "implementationNotes": "keyed, vdom, no-jsx, no-memo, shallow-refs"
      }
    ]
  }
}
